{
    "env": {
        "jasmine": true,
        "node": true,
        "mocha": true,
        "builtin": true
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "globals": {
        "Promise": false
    },
    "rules": {
        "block-scoped-var": 1,
        "comma-style": [
            2,
            "last"
        ],
        "complexity": 1,
        "curly": [
            2,
            "all"
        ],
        "dot-notation": [
            2,
            {
                "allowKeywords": true
            }
        ],
        "eqeqeq": [
            2,
            "allow-null"
        ],
        "guard-for-in": 2,
        "new-cap": 1,
        "no-bitwise": 2,
        "no-caller": 2,
        "no-cond-assign": [
            2,
            "except-parens"
        ],
        "no-debugger": 2,
        "no-empty": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-parens": 1,
        "no-irregular-whitespace": 2,
        "no-iterator": 2,
        "no-loop-func": 2,
        "no-multi-str": 0,
        "no-new": 2,
        "no-proto": 2,
        "no-script-url": 2,
        "no-sequences": 2,
        "no-undef": 2,
        "no-unused-vars": 1,
        "no-with": 2,
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "never"
        ],
        "strict": 2,
        "valid-typeof": 2,
        "wrap-iife": [
            2,
            "inside"
        ]
    }
}